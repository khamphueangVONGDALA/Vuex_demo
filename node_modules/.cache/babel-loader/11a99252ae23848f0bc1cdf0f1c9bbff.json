{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Card from \"./Card.vue\";\nexport default {\n  components: {\n    Card\n  },\n\n  setup() {\n    const store = useStore();\n    const data = computed(() => store.state.arr);\n\n    const handleDelete = i => {\n      store.dispatch(\"delete\", i);\n    };\n\n    const handleEdit = i => {\n      store.dispatch(\"change_index\", i);\n      store.dispatch(\"edit\", data.value[i]);\n    };\n\n    return {\n      data,\n      handleDelete,\n      handleEdit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmDA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,OAAOC,IAAP,MAAiB,YAAjB;AAEA,eAAe;EACbC,UAAU,EAAE;IAAED;EAAF,CADC;;EAEXE,KAAK,GAAG;IACR,MAAMC,KAAI,GAAIJ,QAAQ,EAAtB;IAEA,MAAMK,IAAG,GAAIN,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAN,CAAYC,GAAnB,CAArB;;IAEA,MAAMC,YAAW,GAAKC,CAAD,IAAO;MAC1BL,KAAK,CAACM,QAAN,CAAe,QAAf,EAAyBD,CAAzB;IACD,CAFD;;IAIA,MAAME,UAAS,GAAKF,CAAD,IAAO;MACxBL,KAAK,CAACM,QAAN,CAAe,cAAf,EAA+BD,CAA/B;MACAL,KAAK,CAACM,QAAN,CAAe,MAAf,EAAuBL,IAAI,CAACO,KAAL,CAAWH,CAAX,CAAvB;IACD,CAHD;;IAKA,OAAO;MAAEJ,IAAF;MAAQG,YAAR;MAAsBG;IAAtB,CAAP;EACD;;AAjBY,CAAf","names":["computed","useStore","Card","components","setup","store","data","state","arr","handleDelete","i","dispatch","handleEdit","value"],"sourceRoot":"","sources":["/Users/zero_bot/Documents/Vuejs/blogs/src/components/custom/custom_component.vue"],"sourcesContent":["<template>\n<div>\n    <v-card\n      v-for=\"(item, index) in data\"\n      :key=\"index\"\n      class=\"w-[600px] mx-auto h-auto mt-3\"\n    >\n      <Card>\n        <template v-slot:card-content>\n          <div class=\"flex justify-between py-3 px-3\">\n            <div>\n              <p class=\"text-blue-500\">Name</p>\n              <p>{{ item.name }}</p>\n            </div>\n            <div>\n              <p class=\"text-blue-500\">Title</p>\n              <p>{{ item.title }}</p>\n            </div>\n\n            <div>\n              <p class=\"text-blue-500\">Genre</p>\n              <div>\n                <ul>\n                  <li v-for=\"(i, index) in item.Category\" :key=\"index\">\n                    {{ i }}\n                  </li>\n                </ul>\n              </div>\n            </div>\n          </div>\n        </template>\n\n        <template v-slot:card-button>\n          <div class=\"flex justify-center py-3\">\n            <v-btn color=\"error\" @click=\"handleDelete(item.id)\"> Delete </v-btn>\n            <div class=\"ml-3\">\n              <v-btn tile color=\"success\" @click=\"handleEdit(index)\">\n                <v-icon left> mdi-pencil </v-icon>\n                Edit\n              </v-btn>\n            </div>\n          </div>\n        </template>\n      </Card>\n    </v-card>\n</div>\n\n  \n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Card from \"./Card.vue\";\n\nexport default {\n  components: { Card },\n    setup() {\n    const store = useStore();\n\n    const data = computed(() => store.state.arr);\n\n    const handleDelete = (i) => {\n      store.dispatch(\"delete\", i);\n    };\n\n    const handleEdit = (i) => {\n      store.dispatch(\"change_index\", i);\n      store.dispatch(\"edit\", data.value[i]);\n    };\n\n    return { data, handleDelete, handleEdit };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}