{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"CusTom\",\n\n  setup() {\n    const store = useStore();\n    const data = computed(() => store.state.arr);\n    return {\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;EACbC,IAAI,EAAE,QADO;;EAGbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,EAAtB;IACA,MAAMI,IAAG,GAAIL,QAAQ,CAAC,MAAMI,KAAK,CAACE,KAAN,CAAYC,GAAnB,CAArB;IAEA,OAAO;MAAEF;IAAF,CAAP;EACD;;AARY,CAAf","names":["computed","useStore","name","setup","store","data","state","arr"],"sourceRoot":"","sources":["/Users/zero_bot/Documents/Vuejs/blogs/src/views/custom.vue"],"sourcesContent":["<template>\n  <div class=\"text-center py-3 uppercase\">\n      <h1 >List Customs</h1>\n    <div>\n      <v-card\n        v-for=\"(item, index) in data\"\n        :key=\"index\"\n        class=\"w-[600px] mx-auto h-auto mt-3 hover:bg-green-500 hover:text-white\"\n      >\n<!-- `/custom/${item.id}` -->\n      <router-link :to=\"{name:'custom_detail',params:{id:item.id,name:item.name,title:item.title,blog:item.blog,age:item.age,gender:item.gender}}\">\n        <div class=\"flex justify-between py-3 px-3 items-center\">\n          <p>Name: {{ item.name }}</p>\n\n          <p>Age: {{ item.age }}</p>\n\n          <p>Title: {{ item.title }}</p>\n        </div>\n      </router-link>\n\n      </v-card>\n    </div>\n    <router-view />\n\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  name: \"CusTom\",\n\n  setup() {\n    const store = useStore();\n    const data = computed(() => store.state.arr);\n\n    return { data };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}