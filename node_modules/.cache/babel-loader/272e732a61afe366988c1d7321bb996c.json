{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n    const data = computed(() => store.state.arr);\n\n    const handleDelete = i => {\n      //  data.value = data.value.filter((e)=>e.id !== i)\n      store.dispatch('delete', i);\n    };\n\n    return {\n      data,\n      handleDelete\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA0CA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,EAAtB;IAEA,MAAMG,IAAG,GAAIJ,QAAQ,CAAC,MAAMG,KAAK,CAACE,KAAN,CAAYC,GAAnB,CAArB;;IAEA,MAAMC,YAAW,GAAKC,CAAD,IAAO;MAC5B;MACAL,KAAK,CAACM,QAAN,CAAe,QAAf,EAAwBD,CAAxB;IACC,CAHD;;IAKA,OAAO;MAAEJ,IAAF;MAAQG;IAAR,CAAP;EACD;;AAZY,CAAf","names":["computed","useStore","setup","store","data","state","arr","handleDelete","i","dispatch"],"sourceRoot":"","sources":["/Users/zero_bot/Documents/Vuejs/blogs/src/components/custom/listCustom.vue"],"sourcesContent":["<template>\n  <div>\n    <v-card\n      v-for=\"(item, index) in data\"\n      :key=\"index\"\n      class=\"w-[600px] mx-auto h-auto mt-3\"\n    >\n      <div class=\"flex justify-between py-3 px-3\">\n        <div>\n          <p class=\"text-blue-500\">Name</p>\n          <p>{{ item.name }}</p>\n        </div>\n        <div>\n          <p class=\"text-blue-500\">Title</p>\n          <p>{{ item.title }}</p>\n        </div>\n\n        <div>\n          <p class=\"text-blue-500\">Genre</p>\n          <div>\n            <ul>\n              <li v-for=\"(i, index) in item.Category\" :key=\"index\">\n                {{ i }}\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n      <div class=\"flex justify-center py-3\">\n        <v-btn color=\"error\" @click=\"handleDelete(item.id)\"> Delete </v-btn>\n        <div class=\"ml-3\">\n          <v-btn tile color=\"success\">\n            <v-icon left> mdi-pencil </v-icon>\n            Edit\n          </v-btn>\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n\n    const data = computed(() => store.state.arr);\n\n    const handleDelete = (i) => {\n    //  data.value = data.value.filter((e)=>e.id !== i)\n    store.dispatch('delete',i)\n    };\n\n    return { data, handleDelete };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}