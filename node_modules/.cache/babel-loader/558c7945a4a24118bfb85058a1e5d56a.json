{"ast":null,"code":"import { reactive, computed } from \"vue\";\nimport { v4 as uuidv4 } from \"uuid\";\nimport { useStore } from \"vuex\";\nimport FormInput from \"../components/form/FormInput.vue\";\nexport default {\n  name: 'AdmIn',\n  components: {\n    FormInput\n  },\n\n  setup() {\n    const store = useStore();\n\n    const HandleSubmitForm = () => {\n      //const data2 = Object.assign({}, infomation, {id: uuidv4()});\n      infomation.id = uuidv4();\n      store.dispatch(\"savedata\", infomation);\n      alert(\"Add New Success!!!\");\n    };\n\n    const data = computed(() => store.state.arr);\n    return {\n      data,\n      infomation,\n      HandleSubmitForm\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAOA,SAASA,QAAT,EAAmBC,QAAnB,QAAmC,KAAnC;AACA,SAASC,EAAC,IAAKC,MAAf,QAA6B,MAA7B;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,OAAOC,SAAP,MAAsB,kCAAtB;AAEA,eAAe;EACbC,IAAI,EAAC,OADQ;EAEbC,UAAU,EAAE;IACVF;EADU,CAFC;;EAKbG,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,EAAtB;;IAIA,MAAMM,gBAAe,GAAI,MAAM;MAC7B;MACAC,UAAU,CAACC,EAAX,GAAgBT,MAAM,EAAtB;MACAM,KAAK,CAACI,QAAN,CAAe,UAAf,EAA2BF,UAA3B;MAEAG,KAAK,CAAC,oBAAD,CAAL;IACD,CAND;;IAQA,MAAMC,IAAG,GAAId,QAAQ,CAAC,MAAMQ,KAAK,CAACO,KAAN,CAAYC,GAAnB,CAArB;IAEA,OAAO;MAAEF,IAAF;MAAQJ,UAAR;MAAoBD;IAApB,CAAP;EACD;;AArBY,CAAf","names":["reactive","computed","v4","uuidv4","useStore","FormInput","name","components","setup","store","HandleSubmitForm","infomation","id","dispatch","alert","data","state","arr"],"sourceRoot":"","sources":["/Users/zero_bot/Documents/Vuejs/blogs/src/views/admin.vue"],"sourcesContent":["<template>\n  <div class=\"mt-[20px]\">\n    <FormInput />\n  </div>\n</template>\n\n<script>\nimport { reactive, computed } from \"vue\";\nimport { v4 as uuidv4 } from \"uuid\";\nimport { useStore } from \"vuex\";\nimport FormInput from \"../components/form/FormInput.vue\";\n\nexport default {\n  name:'AdmIn',\n  components: {\n    FormInput,\n  },\n  setup() {\n    const store = useStore();\n\n\n\n    const HandleSubmitForm =