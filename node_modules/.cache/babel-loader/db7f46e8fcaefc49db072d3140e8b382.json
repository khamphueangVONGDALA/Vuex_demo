{"ast":null,"code":"import { computed } from \"vue\";\nimport { v4 as uuidv4 } from \"uuid\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n    const infomation = computed(() => store.state.infomation);\n\n    const HandleSubmitForm = () => {\n      //const data2 = Object.assign({}, infomation, {id: uuidv4()});\n      infomation.value.id = uuidv4();\n      store.dispatch(\"savedata\", infomation.value); // alert(\"Add New Success!!!\");\n    }; // const HandleEdit = () => {\n    //   store.dispatch(\"update\", index.value);\n    // };\n\n\n    return {\n      infomation,\n      HandleSubmitForm\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiHA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,EAAC,IAAKC,MAAf,QAA6B,MAA7B;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,EAAtB;IACA,MAAMG,UAAS,GAAIN,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAN,CAAYD,UAAnB,CAA3B;;IAEA,MAAME,gBAAe,GAAI,MAAM;MAC7B;MACAF,UAAU,CAACG,KAAX,CAAiBC,EAAjB,GAAsBR,MAAM,EAA5B;MACAG,KAAK,CAACM,QAAN,CAAe,UAAf,EAA2BL,UAAU,CAACG,KAAtC,EAH6B,CAI7B;IACD,CALD,CAJM,CAWN;IACA;IAEA;;;IAEA,OAAO;MAAEH,UAAF;MAAcE;IAAd,CAAP;EACD;;AAlBY,CAAf","names":["computed","v4","uuidv4","useStore","setup","store","infomation","state","HandleSubmitForm","value","id","dispatch"],"sourceRoot":"","sources":["/Users/zero_bot/Documents/Vuejs/blogs/src/components/form/FormInput.vue"],"sourcesContent":["<template>\n  <v-card class=\"max-w-[500px] mx-auto px-3 py-3 text-center\">\n    <h1 class=\"py-3 uppercase\">Infomation</h1>\n    <form class=\"\">\n      <div class=\"form-control\">\n        <div>\n          <v-text-field\n            label=\"Name:\"\n            type=\"text\"\n            hide-details=\"auto\"\n            v-model=\"infomation.name\"\n          ></v-text-field>\n        </div>\n      </div>\n\n      <div class=\"form-control\">\n        <div>\n          <v-text-field\n            label=\"Age:\"\n            e=\"text\"\n            hide-details=\"auto\"\n            type=\"Number\"\n            v-model=\"infomation.age\"\n          ></v-text-field>\n        </div>\n      </div>\n\n      <div class=\"form-control\">\n        <div>\n          <v-radio\n            color=\"secondary\"\n            label=\"male\"\n            type=\"checkbox\"\n            v-model=\"infomation.gender\"\n            value=\"male\"\n          ></v-radio>\n        </div>\n        <div>\n          <v-radio\n            color=\"secondary\"\n            label=\"female\"\n            type=\"checkbox\"\n            v-model=\"infomation.gender\"\n            value=\"female\"\n          ></v-radio>\n        </div>\n      </div>\n\n      <div class=\"flex justify-between\">\n        <div class=\"form-control\">\n          <p>Genre</p>\n\n          <div>\n            <v-radio\n              color=\"secondary\"\n              label=\"Xã hội\"\n              type=\"checkbox\"\n              value=\"Xã hội\"\n              v-model=\"infomation.Category\"\n            ></v-radio>\n          </div>\n          <div>\n            <v-radio\n              color=\"secondary\"\n              label=\"Khoa học\"\n              type=\"checkbox\"\n              value=\"Khoa học\"\n              v-model=\"infomation.Category\"\n            ></v-radio>\n          </div>\n          <div>\n            <v-radio\n              color=\"secondary\"\n              label=\"khám phá\"\n              type=\"checkbox\"\n              value=\"khám phá\"\n              v-model=\"infomation.Category\"\n            ></v-radio>\n          </div>\n        </div>\n      </div>\n      <div class=\"form-control\">\n        <div>\n          <v-text-field\n            label=\"Title\"\n            type=\"text\"\n            hide-details=\"auto\"\n            v-model=\"infomation.title\"\n          ></v-text-field>\n        </div>\n      </div>\n      <div>\n        <v-textarea\n          outlined\n          name=\"input-7-4\"\n          label=\"Write Blog\"\n          v-model=\"infomation.blog\"\n        ></v-textarea>\n      </div>\n\n      <div class=\"flex justify-center\">\n        <v-btn depressed color=\"primary\" @click.prevent=\"HandleSubmitForm\"\n          >Submit</v-btn\n        >\n        <!-- <v-btn depressed color=\"primary\" @click.prevent=\"HandleEdit\"\n          >Update</v-btn\n        > -->\n      </div>\n    </form>\n  </v-card>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { v4 as uuidv4 } from \"uuid\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n    const infomation = computed(() => store.state.infomation);\n\n    const HandleSubmitForm = () => {\n      //const data2 = Object.assign({}, infomation, {id: uuidv4()});\n      infomation.value.id = uuidv4();\n      store.dispatch(\"savedata\", infomation.value);\n      // alert(\"Add New Success!!!\");\n    };\n\n    // const HandleEdit = () => {\n    //   store.dispatch(\"update\", index.value);\n\n    // };\n\n    return { infomation, HandleSubmitForm};\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}