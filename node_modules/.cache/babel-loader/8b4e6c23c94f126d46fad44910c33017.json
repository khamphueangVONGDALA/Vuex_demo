{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n    const data = computed(() => store.state.arr); // const infomation = computed(() => store.state.infomation);\n\n    var d = computed(() => store.state.d);\n\n    const handleDelete = i => {\n      store.dispatch(\"delete\", i);\n    };\n\n    const handleEdit = i => {\n      // console.log(data.value[i]);\n      // emit('ud',data.value[i])\n      store.dispatch(\"C\", i); // store.dispatch(\"update\")\n\n      store.dispatch(\"edit\", data.value[i]); // store.dispatch('update',i)\n    };\n\n    return {\n      data,\n      handleDelete,\n      handleEdit,\n      d\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA0CA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,EAAtB;IAEA,MAAMG,IAAG,GAAIJ,QAAQ,CAAC,MAAMG,KAAK,CAACE,KAAN,CAAYC,GAAnB,CAArB,CAHM,CAIN;;IACA,IAAIC,IAAIP,QAAQ,CAAC,MAAMG,KAAK,CAACE,KAAN,CAAYE,CAAnB,CAAhB;;IAEA,MAAMC,YAAW,GAAKC,CAAD,IAAO;MAC1BN,KAAK,CAACO,QAAN,CAAe,QAAf,EAAyBD,CAAzB;IACD,CAFD;;IAIA,MAAME,UAAS,GAAKF,CAAD,IAAO;MACxB;MAEA;MAEAN,KAAK,CAACO,QAAN,CAAe,GAAf,EAAmBD,CAAnB,EALwB,CAMxB;;MACAN,KAAK,CAACO,QAAN,CAAe,MAAf,EAAuBN,IAAI,CAACQ,KAAL,CAAWH,CAAX,CAAvB,EAPwB,CAQxB;IACD,CATD;;IAWA,OAAO;MAAEL,IAAF;MAAQI,YAAR;MAAsBG,UAAtB;MAAkCJ;IAAlC,CAAP;EACD;;AAxBY,CAAf","names":["computed","useStore","setup","store","data","state","arr","d","handleDelete","i","dispatch","handleEdit","value"],"sourceRoot":"","sources":["/Users/zero_bot/Documents/Vuejs/blogs/src/components/custom/listCustom.vue"],"sourcesContent":["<template>\n  <div>\n    <v-card\n      v-for=\"(item, index) in data\"\n      :key=\"index\"\n      class=\"w-[600px] mx-auto h-auto mt-3\"\n    >\n      <div class=\"flex justify-between py-3 px-3\">\n        <div>\n          <p class=\"text-blue-500\">Name</p>\n          <p>{{ item.name }}</p>\n        </div>\n        <div>\n          <p class=\"text-blue-500\">Title</p>\n          <p>{{ item.title }}</p>\n        </div>\n\n        <div>\n          <p class=\"text-blue-500\">Genre</p>\n          <div>\n            <ul>\n              <li v-for=\"(i, index) in item.Category\" :key=\"index\">\n                {{ i }}\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n      <div class=\"flex justify-center py-3\">\n        <v-btn color=\"error\" @click=\"handleDelete(item.id)\"> Delete </v-btn>\n        <div class=\"ml-3\">\n          <v-btn tile color=\"success\" @click=\"handleEdit(index)\">\n            <v-icon left> mdi-pencil </v-icon>\n            Edit\n          </v-btn>\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n\n    const data = computed(() => store.state.arr);\n    // const infomation = computed(() => store.state.infomation);\n    var d = computed(() => store.state.d);\n\n    const handleDelete = (i) => {\n      store.dispatch(\"delete\", i);\n    };\n\n    const handleEdit = (i) => {\n      // console.log(data.value[i]);\n\n      // emit('ud',data.value[i])\n     \n      store.dispatch(\"C\",i)\n      // store.dispatch(\"update\")\n      store.dispatch(\"edit\", data.value[i]);\n      // store.dispatch('update',i)\n    };\n\n    return { data, handleDelete, handleEdit, d };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}